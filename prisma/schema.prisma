generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
 }

model User {
  id         String   @id @default(cuid())
  authUserId String   @unique
  email      String
  name       String?
  avatarUrl  String?

  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  
   @@map("users")
}